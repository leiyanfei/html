extends component-box

block css
  link(rel="stylesheet",type="text/css", href="#{webtop}css/index.css")
  link(rel="stylesheet",type="text/css", href="//at.alicdn.com/t/font_706810_4im35j13prp.css")
  
block content
  .page_zs
    header.page-header
      .header-main.zz-flexbox
        a.header-left.container(href="javascript:void(0)" onclick="javascript :history.back(-1)")
          .block_icon
        .header-content.zz-flexbox-item.container 展商指南
        .header-right.container
          a.bm_btn(href="baoming.html") 报名
    block zs_content
      .zs_nav
        //- ,{classname:"znav7",navname:'参展商手册',gohref:"zs_detail_04.html"},{classname:"znav9",navname:'供需展厅',gohref:"zn_gongxu.html"}
        - var navlist = [{classname:"znav6",navname:'参展申请',gohref:"baoming.html"},{classname:"znav2",navname:'展位分布图',gohref:"zn_fbpic.html"},{classname:"znav8",navname:'展商列表',gohref:"zn_zhanshang.html"}]
        each item in navlist
          a(href="#{item.gohref}").zs_nav_li.zz-flexbox
            .zs_icon_box.container
                i(class="zs_icon #{item.classname}")
            .zz-flexbox-item.zs_nav_name.vdmiddle #{item.navname}
            .zs_right_go
              .right_icon
        

  
    - var navindex = 2
    include component-navbt